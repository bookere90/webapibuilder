{
    "name": "WebAPIBuilder",
    "description": "an extension to build simple to complex webAPI queries in a simple-to-use, compact interface",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "main.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "bookmarks",
        "unlimitedStorage",
        "storage",
        "tabs"
    ],
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": [
            "jquery.js",
            "main.js"
        ]
    } ]
}